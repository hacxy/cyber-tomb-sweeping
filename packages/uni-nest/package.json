    "@nestjs/schematics": "^10.0.0",
    "@nestjs/swagger": "^7.1.16",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
